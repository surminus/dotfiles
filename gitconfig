[push]
	default = simple
[alias]
	st = status
	ci = commit
	br = branch
	co = checkout
	pick-axe = log -p -S
	squash = rebase -i --autosquash
	fixup = commit --fixup
	list-commit = log --pretty=oneline --abbrev-commit
	nuke = reset --hard origin/master
        pr   = pull-request
        pushf = push -f origin head
        findbranch = branch | grep

