[push]
  default = simple

[alias]
  st = status
  c = commit -v
  br = branch
  co = !sh -c 'git stash; git stash apply; git checkout "$@"'
  tidy = clean -fd .
  pick-axe = log -p -S
  squash = rebase -i --autosquash
  fixup = commit --fixup
  nuke = reset --hard origin/master
  last = --no-pager log --oneline -10
  pr   = pull-request
  pushf = push --force-with-lease origin HEAD
  ammend = commit --amend --no-edit

[pull]
  rebase = preserve

[add]
  patch = true

